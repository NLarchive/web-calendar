{
  "id": "pregnancy-care",
  "title": "Pregnancy Care Calendar Calculator",
  "description": "Data-driven pregnancy timeline from conception (week 0) through postpartum follow-up.",
  "disclaimer": "Educational schedule template only. Clinical timing must be personalized by your OB/GYN or healthcare team.",
  "fields": [
    { "name": "personName", "label": "Person name", "type": "text", "required": false, "placeholder": "Ana" },
    { "name": "conceptionDate", "label": "Conception date", "type": "date", "required": true },
    {
      "name": "highRisk",
      "label": "Risk profile",
      "type": "select",
      "required": true,
      "defaultValue": "standard",
      "options": [
        { "value": "standard", "label": "Standard" },
        { "value": "high", "label": "High-risk follow-up" }
      ]
    },
    {
      "name": "country",
      "label": "Country / Region Guidelines",
      "type": "select",
      "required": true,
      "defaultValue": "global",
      "options": [
        { "value": "global", "label": "Global (WHO/NICE/ACOG)" },
        { "value": "chile", "label": "Chile (MINSAL public schedule)" }
      ]
    },
    { "name": "lastTdapDate", "label": "Last Tdap date (optional)", "type": "date", "required": false }
  ],
  "derivedDates": [
    { "name": "dueDate", "fromField": "conceptionDate", "addDays": 280 },
    { "name": "postpartumDate", "fromDerived": "dueDate", "addDays": 42 }
  ],
  "calculationWindow": {
    "startAnchor": "conceptionDate",
    "endAnchor": "postpartumDate",
    "futureOnly": true
  },
  "events": [
    {
      "id": "prenatal-initial",
      "titleTemplate": "First prenatal consultation{entitySuffix}",
      "description": "Initial prenatal evaluation and baseline care plan.",
      "category": "pregnancy",
      "tags": ["pregnancy", "prenatal", "consultation"],
      "priority": 9,
      "reminderMinutes": 1440,
      "schedule": { "mode": "single", "anchor": "conceptionDate", "offsetWeeks": 8 }
    },
    {
      "id": "dating-ultrasound",
      "titleTemplate": "Dating ultrasound (11-14 weeks){entitySuffix}",
      "description": "Global recommendation: dating scan in late first trimester with optional combined screening.",
      "category": "pregnancy",
      "tags": ["pregnancy", "ultrasound"],
      "priority": 8,
      "reminderMinutes": 1440,
      "conditions": [
        { "field": "country", "in": ["global"] }
      ],
      "schedule": { "mode": "single", "anchor": "conceptionDate", "offsetWeeks": 10 }
    },
    {
      "id": "dating-ultrasound-chile",
      "titleTemplate": "Early ultrasound (Chile pathway){entitySuffix}",
      "description": "Chile pathway: perform early pregnancy ultrasound before week 20, ideally first trimester when available.",
      "category": "pregnancy",
      "tags": ["pregnancy", "ultrasound", "chile"],
      "priority": 8,
      "reminderMinutes": 1440,
      "conditions": [
        { "field": "country", "in": ["chile"] }
      ],
      "schedule": { "mode": "single", "anchor": "conceptionDate", "offsetWeeks": 12 }
    },
    {
      "id": "nuchal-translucency",
